<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Document</title>
    <link rel="stylesheet" href="/assets/showCards.css" />
  </head>
  <body>
    <header>showCards</header>
    <main>
      <% cards.forEach(card => { %>
      <div>
        <h2>Card</h2>
        <p>Title: <%= card.title %></p>
        <p>Description: <%= card.description %></p>
        <p>Image: <img src="assets/avatar.png" alt="Card Image" /></p>
      </div>
      <% }); %>
    </main>
  </body>
</html>
